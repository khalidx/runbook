{
  "name": "@khalidx/runbook",
  "version": "0.0.0",
  "description": "Executable markdown documents that you can run, template, and share!",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "run-s build:node",
    "build": "run-s build:*",
    "build:clean": "rimraf dist/ exec/",
    "build:node": "tsc",
    "build:native": "pkg . --out-path exec/"
  },
  "files": [
    "dist/",
    "logo.png"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "handlebars": "^4.7.6",
    "remark": "^13.0.0",
    "unist-util-visit": "^2.0.3",
    "uuid": "^8.3.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.11",
    "npm-run-all": "^4.1.5",
    "pkg": "^4.4.9",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
